generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  age        Int
  status     Boolean
  familyType String
  createdAt  DateTime @default(now())
}
